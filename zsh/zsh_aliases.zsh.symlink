# ~/.dotfiles/zsh/zsh_aliases.zsh.symlink


# === Aliases ===

# System & utilities
alias bundle='brew bundle --file=~/.bootstrap/Brewfile'
alias c='clear'
alias lns='find . -type l'
alias ls='ls -lG'
alias lh='ls -ld .??*'
alias ll='ls -lah'
alias r='source ~/.zshrc'
alias x='exit'

# Navigation
alias ..='cd ..'
alias ...='cd ../..'

# Git
alias ga='git add .'
alias gc='git commit -v'
alias gl='git pull'
alias gp='git push'
alias gs='git status'